  "scripts": {
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "start-server": "npx http-server src -p 3001",

    "cypress": "cypress open --browser chrome",
    "tests": "npm run start-server & cypress run --browser chrome && kill $!",
    "dev-tests": "npm run start-server & npm run cypress",

    "deploy": "gh-pages -d dist/site -m 'Deploy new version' -b gh-pages",

    "jekyll build incremental": "jekyll build --source site --destination dist/site --config site/_config.yml --incremental",
    "jekyll serve": "jekyll serve --port 8080 --source site --destination dist/site --config site/_config.yml --incremental",
    "jekyll build": "jekyll build --source site --destination dist/site --config site/_config.yml"
  },
  "jekyll": {
    "gems": [
      "jekyll",
      "другие_гемы_если_нужно"
    ],
    "build_dir": ".build"
